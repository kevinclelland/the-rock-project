---
title: News
layout: base.njk
---

<div class="container">
    <h1>News</h1>
    <div class="vibe">You ROCK!</div>
    <div class="love">
        <i class="fas fa-headphones"></i> ❤️ <i class="fas fa-guitar"></i>
      
    </div>

    <ul>
  {% for article in news.articles %}
    <li>
      <h2><a href="{{ article.url }}">{{ article.title }}</a></h2>
      <span>{{ article.source.name }} - <time>{{ article.publishedAt }}</time></span>
    </li>

  {% endfor %}
  </ul>

  </div>
  <script src="/js/vibe.js"></script>